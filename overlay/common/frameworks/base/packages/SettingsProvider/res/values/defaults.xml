<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Install other apps -->
    <bool name="def_install_non_market_apps">true</bool>
    <!-- Dont verify apps -->
    <bool name="def_package_verifier_enable">false</bool>

    <!-- Keep wifi awake -->
    <integer name="def_wifi_sleep_policy">0</integer>

    <!-- No lockscreen if keyguard comp. enabled -->
    <bool name="def_lockscreen_disabled">true</bool>
    <!-- No provisioning -->
    <bool name="def_device_provisioned">true</bool>

    <!-- Landscape -->
    <integer name="def_user_rotation">3</integer>
</resources>